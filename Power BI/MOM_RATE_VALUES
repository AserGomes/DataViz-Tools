MOM_%RATES = 
//DATA DE REFERÊNCIA
VAR DATA_ATT = MONTH(MAX(FUNIL_EMAIL[DATA]))
//ANO DE REFERÊNCIA
VAR YEAR_ATT = YEAR(MAX(FUNIL_EMAIL[DATA]))
//VALOR DE REFERÊNCIA TEMPO(t)
VAR M_ATT = CALCULATE(AVERAGE(FUNIL_EMAIL[%RATES]), REMOVEFILTERS(),
    YEAR(FUNIL_EMAIL[DATA]) = YEAR_ATT, 
    MONTH(FUNIL_EMAIL[DATA]) = DATA_ATT
    )
//VALOR DE REFERÊNCIA TEMPO(t-1)
VAR M_ANT = CALCULATE(AVERAGE(FUNIL_EMAIL[%RATES]),REMOVEFILTERS(),
    YEAR(FUNIL_EMAIL[DATA]) = IF(DATA_ATT = 12 , YEAR_ATT-1, YEAR_ATT), //CORREÇÃO DE CICLO 12 MESES
    MONTH(FUNIL_EMAIL[DATA]) = IF(DATA_ATT = 12, 1, DATA_ATT-1)
    )
//RESULTADO DE DIVISÃO, VALOR ALTERNATIVO = VAZIO
RETURN DIVIDE((M_ATT-M_ANT),M_ANT,BLANK())
